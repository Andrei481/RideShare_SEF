<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegistrationController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="600.0" prefWidth="600.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="400.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <VBox alignment="CENTER" spacing="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <VBox spacing="10.0">
                     <children>
                          <Label prefHeight="22.0" prefWidth="118.0" text="First name">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label prefHeight="22.0" prefWidth="118.0" text="Last name">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label prefHeight="22.0" prefWidth="118.0" text="Age">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label prefHeight="22.0" prefWidth="118.0" text="Gender">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label prefHeight="22.0" prefWidth="118.0" text="Email address">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label text="Username">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label text="Password">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label text="Role">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                          <Label fx:id="labelLicensePlate" text="License plate">
                              <font>
                                  <Font name="Arial Rounded MT Bold" size="17.0" />
                              </font>
                          </Label>
                     </children>
                  </VBox>
                  <VBox spacing="10.0">
                     <children>
                          <TextField fx:id="textFieldFirstName" prefHeight="26.0" prefWidth="187.0" />
                          <TextField fx:id="textFieldLastName" prefHeight="26.0" prefWidth="187.0" />
                          <TextField fx:id="textFieldAge" prefHeight="26.0" prefWidth="46.0" />
                          <ComboBox fx:id="comboBoxGender" prefHeight="26.0" prefWidth="110.0" />
                          <TextField fx:id="textFieldEmail" prefHeight="26.0" prefWidth="187.0" />
                          <TextField fx:id="textFieldUsername" prefHeight="26.0" prefWidth="187.0" />
                          <TextField fx:id="textFieldPassword" prefHeight="26.0" prefWidth="187.0" />
                        <HBox spacing="20.0">
                           <children>
                                <RadioButton fx:id="radioButtonDriver" mnemonicParsing="false" text="Driver">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </RadioButton>
                                <RadioButton fx:id="radioButtonClient" mnemonicParsing="false" text="Client">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </RadioButton>
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </HBox>
                          <TextField fx:id="textFieldLicensePlate" prefHeight="26.0" prefWidth="187.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                    <Button fx:id="buttonLogin" mnemonicParsing="false" style="-fx-background-color: #bfbfbf; -fx-cursor: hand;" text="Cancel">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Button>
                    <Button fx:id="buttonSignup" mnemonicParsing="false" prefHeight="27.0" prefWidth="125.0" style="-fx-background-color: #1362f5; -fx-cursor: hand;" text="Sign up" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="18.0" />
                        </font>
                    </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
