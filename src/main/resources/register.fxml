<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegistrationLoginController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="600.0" prefWidth="600.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="540.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <VBox alignment="CENTER" minHeight="540.0" minWidth="500.0" spacing="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="CREATE YOUR ACCOUNT" textAlignment="CENTER" textFill="#31c6a3">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <HBox alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="First name">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldFirstName" prefHeight="26.0" prefWidth="187.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Last name">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldLastName" prefHeight="26.0" prefWidth="187.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Age">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldAge" prefHeight="26.0" prefWidth="39.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Gender">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <ComboBox fx:id="comboBoxGender" prefHeight="26.0" prefWidth="82.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Email address">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldEmail" prefHeight="26.0" prefWidth="187.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Username">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldUsername" prefHeight="26.0" prefWidth="187.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Password">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldPassword" prefHeight="26.0" prefWidth="187.0" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label prefHeight="22.0" prefWidth="118.0" text="Role">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                              <HBox spacing="20.0">
                                 <children>
                                      <RadioButton fx:id="radioButtonDriver" mnemonicParsing="false" text="Driver">
                                          <font>
                                              <Font size="17.0" />
                                          </font>
                                      </RadioButton>
                                      <RadioButton fx:id="radioButtonClient" mnemonicParsing="false" text="Client">
                                          <font>
                                              <Font size="17.0" />
                                          </font>
                                      </RadioButton>
                                 </children>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                              </HBox>
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                                <Label fx:id="labelLicensePlate" prefHeight="22.0" prefWidth="118.0" text="License plate">
                                    <font>
                                        <Font size="17.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="textFieldLicensePlate" prefHeight="26.0" prefWidth="85.0" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                    <Button fx:id="buttonLogin" maxWidth="90.0" minWidth="90.0" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: #bfbfbf; -fx-cursor: hand;" text="Back">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Button>
                    <Button fx:id="buttonSignup" maxWidth="125.0" minWidth="131.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="131.0" style="-fx-background-color: #1362f5; -fx-cursor: hand;" text="Continue" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="18.0" />
                        </font>
                    </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
